extends layout/main
include mixins/videoBlock

block content
  .search__header
    h3 Searching for #{searchingBy}
  .search__videos
    if vides.length === 0
      h5 No Videos Found
    each item in vides
      +videoBlock({
          title:item.title,
          views:item.views,
          videoFile:item.videoFile,
          id:item.id
        })
